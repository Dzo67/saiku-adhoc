		<overlay id="startup.saikuAnalysis"> <toolbar id="mainToolbar"> <toolbarbutton 
		id="openSaiku" image="../content/saiku-ui/images/src/saiku_32.png" onclick="mainToolbarHandler.openUrl('Saiku 
		Analytics','Saiku Analytics','content/saiku-ui/index.html?biplugin=true')" 
		tooltiptext="${saiku-plugin.label}" insertafter="newAnalysisButton" /> </toolbar> 
		</overlay> </overlay> </overlays -->

	<menu-items>
		<menu-item id="new-adhoc" anchor="file-new-submenu" label="Saiku Reporting"
			command="content/saiku-adhoc/web/index.html" type="MENU_ITEM" how="LAST_CHILD" />
	</menu-items>

	<content-type type="adhoc" mime-type="text/html">
		<title>Saiku Reporting</title>
		<description>Saiku Reporting File</description>
		<icon-url>content/saiku-ui/images/src/saiku_16.png</icon-url>
		<meta-provider>org.saiku.adhoc.service.AdhocContentTypeMetaProvider
		</meta-provider>
		<operations>
			<operation>
				<id>RUN</id>
				<command>content/saiku-adhoc/web/index.html?solution={solution}&amp;path={path}&amp;action={name}&amp;biplugin=true#query/open/{name}
				</command>
			</operation>
			<operation>
				<id>EDIT</id>
				<command>content/saiku-adhoc/web/index.html?solution={solution}&amp;path={path}&amp;action={name}&amp;biplugin=true#query/open/{name}
				</command>
			</operation>
			<operation>
				<id>NEWWINDOW</id>
				<command>content/saiku-adhoc/web/index.html?solution={solution}&amp;path={path}&amp;action={name}&amp;biplugin=true#query/open/{name}
				</command>
			</operation>
		</operations>
	</content-type>


	<static-paths>
		<static-path url="/saiku-adhoc/web" localFolder="web" />
		<static-path url="/saiku-adhoc/web/js/templates"
			localFolder="web/js/templates" />
		<static-path url="/saiku-adhoc/web/js" localFolder="web/js" />
		<static-path url="/saiku-adhoc/resources" localFolder="resources/templates" />
		<static-path url="/saiku-adhoc/web/css" localFolder="web/css" />
		<static-path url="/saiku-adhoc/web/images" localFolder="web/images" />


	</static-paths>

	<content-generator scope="session" id="adhoc-service"
		type="adhoc-service">
		<classname>org.saiku.adhoc.service.SaikuAdhocContentGenerator
		</classname>
		<title>saiku-adhoc</title>
	</content-generator>

	<content-generator id="saiku-adhoc" title="fa la la la la, la la la la"
		type="notused" class="org.saiku.adhoc.service.ServletAdapterContentGenerator" />

	<lifecycle-listener class="org.saiku.adhoc.utils.AdhocLifecycleListener" />

</plugin>
